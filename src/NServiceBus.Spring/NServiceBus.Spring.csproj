<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net452</TargetFrameworks>
    <AssemblyName>NServiceBus.ObjectBuilder.Spring</AssemblyName>
    <NuSpecFileName>NServiceBus.Spring</NuSpecFileName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <NoWarn>CS0419</NoWarn>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
    <RootNamespace>NServiceBus.ObjectBuilder.Spring</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <Description>The Spring Container for the nservicebus</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>NServiceBus.Spring</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Spring.Core" Version="[2.0.1, 3.0.0)" />
    <PackageReference Include="Janitor.Fody" Version="1.*" PrivateAssets="all" />
    <PackageReference Include="NServiceBus" Version="[7.0.0-*, 8.0.0)" />
    <PackageReference Include="Obsolete.Fody" Version="4.*" PrivateAssets="all" />
    <PackageReference Include="Fody" Version="2.*" PrivateAssets="all" />
    <None Include="..\..\GitVersion.yml" Link="GitVersion.yml" />
  </ItemGroup>
</Project>
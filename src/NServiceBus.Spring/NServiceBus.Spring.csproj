<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.ObjectBuilder.Spring</AssemblyName>
    <NuSpecFileName>NServiceBus.Spring</NuSpecFileName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <NoWarn>CS0419</NoWarn>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
    <RootNamespace>NServiceBus.ObjectBuilder.Spring</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Common.Logging" Version="3.0.0" />
    <PackageReference Include="Common.Logging.Core" Version="3.0.0" />
    <PackageReference Include="Spring.Core" Version="2.0.1" />
    <PackageReference Include="Janitor.Fody" Version="1.4.1" />
    <PackageReference Include="NServiceBus" Version="6.0.0" />
    <PackageReference Include="Obsolete.Fody" Version="4.2.2" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" />
    <PackageReference Include="NuGetPackager" Version="0.6.3" />
    <PackageReference Include="Fody" Version="2.1.0" />
    <PackageReference Include="GitVersionTask" Version="4.0.0-beta0012" />
    <None Include="..\..\GitVersion.yml" Link="GitVersion.yml" />
    <None Include="..\..\packaging\nuget\NServiceBus.Spring.nuspec" Link="NServiceBus.Spring.nuspec" />
  </ItemGroup>
</Project>